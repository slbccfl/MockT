<div class = "row mockt-page-nav">
   <button class="btn btn-secondary"><%= link_to "Return to List of Rounds for this Competition" , competition_path(@competition) %></button>
   <button class="btn btn-secondary"><%= link_to "Add a Ballot to this Competition Round" , new_competition_round_ballot_path(@competition, @round) %></button>
</div>

<div class="container">
   <h2>Ballots in Round <%= @round.number %></h2>

   <ul class="list-group">
   <% @round_ballots.each do |ballot| %>
   	<li class="list-group-item"><%= link_to "Ballot for judge #{ballot.judge_id} ", ballot_path(ballot.id) %></li>
   <% end %>
   </ul>

</div
