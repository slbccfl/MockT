<div class = "row mockt-page-nav">
	<button class="btn btn-secondary"><%= link_to "Add", new_team_path %></button>
</div>

<div class="container">
	<h1>Mock Trial Teams</h1>
	<ul>
		<% @teams.each do |team| %>
			<% li_item = "#{team.id}. #{team.name}, #{team.institution.name}" %>
			<li class="list-group-item"><%= link_to li_item, edit_team_path(team)%></li>
		<% end %>
	</ul>

</div>